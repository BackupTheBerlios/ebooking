<hibernate-mapping>

    <class name="ebooking.core.menu.MenuItemAuthorization" table="CORE_MENU_ITEM_AUTHORIZATION">

        <id name="id" type="long">
            <column name="ID" not-null="true"/>
            <generator class="increment"/>
        </id>

        <property name="userRole" type="string">
            <column name="USER_ROLE"/>
        </property>

    </class>

</hibernate-mapping>

